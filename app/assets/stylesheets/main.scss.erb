.wf-loading {
  visibility: hidden;
}

.wf-active {
  visibility: visible;
}

#js-warning-text {
  display: none;
}

span.tooltip {
  display:inline-block;
  position: absolute;
  width: 200px;
  height: 200px;
  line-height: 20px;
  padding: 10px;
  font-size: 14px;
  text-align: left;
  color: rgb(113, 157, 171);
  background: #66FF66;
  border: 4px solid #66FF66;
  border-radius: 4px;
  text-shadow: rgba(0, 0, 0, 0.0980392) 1px 1px 1px;
  box-shadow: rgba(0, 0, 0, 0.0980392) 1px 1px 2px 0px;
  opacity: 0.8;
  filter: alpha(opacity=80);
}

span.tooltip:after {
  content: "";
  position: absolute;
  width: 0;
  height: 0;
  border-width: 10px;
  border-style: solid;
  border-color: transparent #66FF66 transparent transparent;
  top: -3px;
  left: -23px;
}
