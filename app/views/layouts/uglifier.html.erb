    }

    div.uglifytooltip .uglifymonotypecorsiva {
      margin-bottom: -5px;
      font-family: "Monotype Corsiva", "Apple Chancery", "Zapf Chancery", Zapf-Chancery, "URW Chancery L", cursive;
    }

    div.uglifytooltip .uglifypapyrus {
