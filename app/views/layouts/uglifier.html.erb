  <style type="text/css">
    body {
      cursor: cell;
    }

    div.uglifyoverlay {
      display: none;
      position: fixed;
      width: 100%;
      height: 100%;
      padding:0;
      margin:0;
      top: 0;
      left: 0;
      visibility: none;
      z-index: 0;
    }

    div.uglifytooltip {
      display: none;
      position: absolute;
      width: 200px;
      height: 200px;
      line-height: 20px;
      padding: 0px;
      font-family: Helvetica, Arial, sans-serif;
      font-size: 24px;
      font-weight: bold;
      background: #66ccff;
      border: 4px solid #66ccff;
      border-radius: 4px;
      box-shadow: rgba(0, 0, 0, 0.0980392) 1px 1px 2px 0px;
      opacity: 0.92;
      filter: alpha(opacity=92);
      cursor: auto;
    }

    div.uglifytooltip:after {
      content: "";
      position: absolute;
      width: 0;
      height: 0;
      border-width: 10px;
      border-style: solid;
      border-color: transparent #66ccff transparent transparent;
      top: -3px;
      left: -23px;
    }

    div.uglifytooltip .uglifydismissbutton {
      display: inline-block;
      float: right;
      width: 22px;
      height: 22px;
      line-height: 0px;
      padding: 0px;
      font-weight: bold;
      text-align: left;
      background: none;
      border: none;
      opacity: 0.60;
      filter: alpha(opacity=60);
    }

    div.uglifytooltip .uglifydismissbutton:hover {
      opacity: 0.80;
      filter: alpha(opacity=80);
    }

    div.uglifytooltip .uglifyminbutton {
