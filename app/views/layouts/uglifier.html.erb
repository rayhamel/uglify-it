  <style type="text/css">
    div.uglifyoverlay {
      display: none;
      position: fixed;
      width: 100%;
      height: 100%;
      padding:0;
      margin:0;
      top: 0;
      left: 0;
      visibility: none;
      z-index: 0;
    }

    div.uglifytooltip {
      display: none;
      position: absolute;
      width: 200px;
      height: 200px;
      line-height: 20px;
      padding: 0px;
      font-family: Helvetica, Arial, sans-serif;
      font-size: 24px;
      font-weight: bold;
      background: #66ccff;
      border: 4px solid #66ccff;
      border-radius: 4px;
      box-shadow: rgba(0, 0, 0, 0.0980392) 1px 1px 2px 0px;
      opacity: 0.92;
      filter: alpha(opacity=92);
    }

    div.uglifytooltip:after {
      content: "";
      position: absolute;
      width: 0;
      height: 0;
      border-width: 10px;
      border-style: solid;
      border-color: transparent #66ccff transparent transparent;
      top: -3px;
      left: -23px;
    }

    div.uglifytooltip .uglifydismissbutton {
      display: inline-block;
      float: right;
      width: 22px;
      height: 22px;
      line-height: 0px;
      padding: 0px;
      font-weight: bold;
      text-align: left;
      background: none;
      border: none;
      opacity: 0.60;
      filter: alpha(opacity=60);
    }

    div.uglifytooltip .uglifydismissbutton:hover {
      opacity: 0.80;
      filter: alpha(opacity=80);
    }

    div.uglifytooltip .uglifyminbutton {
      margin-right: 2px;
      padding-top: 10px;
      font-size: 35px;
    }

    div.uglifytooltip .uglifyclosebutton {
      padding-bottom: 6px;
      font-size: 40px;
    }

    div.uglifytooltip .setmax {
      display: none;
    }

    div.uglifytooltip .uglifytoolbar {
      padding: 0px;
      font-size: 18px;
      font-weight: bold;
      background: none;
      border: none;
      opacity: 0.60;
      filter: alpha(opacity=60);
    }

    div.uglifytooltip .uglifytoolbar:hover {
      opacity: 0.80;
      filter: alpha(opacity=80);
    }

    div.uglifytooltip .uglifytool {
      display: none;
    }

    div.uglifytooltip .uglifyfonts {
      display: inline-block;
    }

    div.uglifytooltip .uglifycolorcontainer {
      position: absolute;
      width: 55px;
      top: 52px;
      left: 140px;
    }

    div.uglifytooltip .uglifybgcontainer {
      position: absolute;
      width: 137px;
      top: 58px;
      left: 3px;
    }

    div.uglifytooltip .uglifyfontselect {
      display: block;
      float: bottom;
      padding: 0px;
      background: none;
      border: none;
      opacity: 0.60;
      filter: alpha(opacity=60);
    }

    div.uglifytooltip .uglifyfontselect:hover {
      opacity: 0.80;
      filter: alpha(opacity=80);
    }

    div.uglifytooltip .uglifycomicsans {
      margin-bottom: -5px;
      font-family: "Comic Sans MS", "Comic Sans", Chalkboard, cursive;
    }

    div.uglifytooltip .uglifycouriernew {
      font-family: "Courier New", Courier, "Liberation Mono", monospace;
    }

